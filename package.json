{
  "name": "harvestr",
  "version": "0.1.0",
  "description": "Harvestr",
  "main": "server/server.js",
  "scripts": {
    "dev": "webpack-dev-server --content-base public/ --hot --inline",
    "knex": "knex",
    "local": "export NODE_ENV=development; node-inspector & nodemon --debug server/server & webpack --watch",
    "start": "node server/server",
    "test": "tape -r babel-register babel-tape-runner 'test/**/*.test.js' | tap-spec",
    "heroku-postbuild": "npm run knex migrate:latest && npm run knex seed:run",
    "api-test": "node client/TEMP-api-postman-testing.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Kahikatea-2016",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "babel-tape-runner": "^2.0.1",
    "enzyme": "^2.3.0",
    "jsx-loader": "^0.13.2",
    "node-inspector": "^0.12.8",
    "nodemon": "^1.10.2",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.4",
    "express-jwt": "^5.1.0",
    "express-session": "^1.14.1",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.12.6",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "6.1.0",
    "react-router": "^2.8.1",
    "sqlite3": "^3.1.6",
    "superagent": "^2.3.0"
  },
  "engines": {
    "node": "6.3.1"
  }
}
